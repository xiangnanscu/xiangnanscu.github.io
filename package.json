{
  "name": "xnscu",
  "version": "0.1.0",
  "type": "module",
  "description": "Xiang Nan's Blog",
  "main": "src/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiangnanscu/xiangnanscu.github.io.git"
  },
  "author": "Xiang Nan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiangnanscu/xiangnanscu.github.io/issues"
  },
  "homepage": "https://xnscu.com/xiangnanscu.github.io/",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "commit": "git add . && git commit -am ",
    "push": "yarn commit",
    "postpush": "while true; do git push origin master && { echo 'Git push succeeded'; break; } || echo 'Git push failed, retrying in 1 seconds'; sleep 1; done",
    "pull": "while true; do git pull origin master && { echo 'Git pull succeeded'; break; } || echo 'Git pull failed, retrying in 1 seconds'; sleep 1; done",
    "prerelease": "npm --no-git-tag-version version minor",
    "release": "node makeDocs.js && push_option=release npm run push",
    "rc": "yarn release"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@tinymce/tinymce-vue": "^5.1.1",
    "@vant/area-data": "^1.5.0",
    "@vant/touch-emulator": "^1.4.0",
    "@vueuse/core": "^10.4.1",
    "@xiangnanscu/xlsx-template": "^1.4.3",
    "ant-design-vue": "^4.0.2",
    "axios": "^1.5.0",
    "docx-templates": "^4.11.3",
    "file-saver": "^2.0.5",
    "highlight.js": "^11.8.0",
    "markdown-it": "^13.0.1",
    "mime-types": "^2.1.35",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.6",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primelocale": "^0.0.1",
    "primevue": "^3.34.1",
    "qrcode": "^1.5.3",
    "stream-browserify": "^3.0.0",
    "vant": "^4.6.7",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@originjs/vite-plugin-require-context": "^1.0.9",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "@xiangnanscu/vite-plugin-alioss": "^1.1.1",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  }
}